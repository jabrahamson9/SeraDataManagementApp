            },
            "ui:order": [
                "experimentType",
                "runType",
                "analyte",
                "tags"
            ]
        },
        "constituent": {
            "title": "CONSTITUENT",
            "type": "array",
            "ui:type": "ConstituentItem",
            "items": {
                "id": "basic",
                "type": "object",
                "ui:order": [
                    "constituent",
                    "strain",
                    "measure1",
                    "measure2"
                ],
                "units": "definitions/units",
                "constituent": "definitions/constituents",
                "properties": {
                    "constituent": {
                        "title": "Pathogen"
                    },
                    "strain": {
                        "title": "Strain"
                    },
                    "measure1": {
                        "title": "measure1"
                    },
                    "measure2": {
                        "title": "measure2"
                    }
                }
            }
        },
        "sample": {
            "title": "SAMPLE",
            "type": "object",
            "properties": {
                "sampleNumber": {
                    "title": "SAMPLE NUMBER",
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 9999,
                    "postfix": "string:sm"
                },
                "cuvetteID": {
                    "title": "CUVETTE ID",
                    "type": "string",
                    "maximum": 16,
                    "postfix": "string:cid"
                },
                "batchNumber": {
                    "title": "BATCH NUMBER",
                    "type": "string",
                    "maximum": 8,
                    "postfix": "string:bn"
                },
                "extension": {
                    "title": "EXTENSION",
                    "type": "string",
                    "maximum": 100
                },
                "notes": {
                    "title": "SAMPLE NOTES",
                    "type": "string",
                    "maximum": 300
                }
            },
            "ui:order": [
                "sampleNumber",
                "cuvetteID",
                "batchNumber",
                "extension",
                "notes"
            ]
        },
        "acquisition": {
            "title": "ACQUISITION",
            "type": "object",
            "properties": {
                "numberOfAcquistions": {
                    "title": "NUMBER OF ACQUISITIONS",
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 500,
                    "default": 50
                },
                "integrationTime": {
                    "title": "INTEGRATION TIME (ms)",
                    "type": "integer",
                    "minimum": 5,
                    "maximum": 15000,
                    "default": 10000,
                    "postfix": "string:ms"
                },
                "acquistionMode": {
                    "title": "ACQUISITION MODE",
                    "type": "string",
                    "default": "Aggregate",
                    "enum": [
                        "Aggregate",
                        "Average",
                        "Continuous"
                    ],
                    "abbreviation": [
                        "aggr",
                        "avg",
                        "con"
                    ]
                },
                "aggregateNumber": {
                    "title": "AGGREGATE NUMBER",
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 200,
                    "default": 6,
                    "postfix": "setting:acquisition.acquistionMode"
                },
                "acquistionDelay": {
                    "title": "ACQUISITION DELAY (ms)",
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 15000,
                    "default": 100,
                    "postfix": "string:msd"
                },
                "cameraFilter": {
                    "title": "CAMERA FILTER (kHz)",
                    "type": "string",
                    "default": "45",
                    "postfix": "string:khz",
                    "enum": [
                        "45",
                        "500"
                    ]
                }
            },
            "required": [
                "numberOfAcquistions",
                "integrationTime",
                "acquistionMode",
                "aggregateNumber",
                "acquistionDelay",
                "cameraFilter"
            ],
            "ui:order": [
                "numberOfAcquistions",
                "integrationTime",
                "acquistionMode",
                "aggregateNumber",
                "acquistionDelay",
                "cameraFilter"
            ]
        }
