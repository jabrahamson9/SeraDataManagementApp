                        "RSP",
                        "BCT",
                        "URN",
                        "FCL",
                        "EXP",
                        "VRS",
                        "ANBC",
                        "CAL",
                        "NET"
                    ],
                    "default": "No Experiment Type"
                },
                "runType": {
                    "title": "RUN TYPE",
                    "type": "string",
                    "enum": [
                        "Regular",
                        "Time Trial",
                        "Overnight",
                        "No Run Type"
                    ],
                    "abbreviation": [
                        "REG",
                        "TMT",
                        "OVN",
                        "NRT"
                    ],
                    "default": "No Run Type"
                },
                "analyte": {
                    "title": "ANALYTE",
                    "type": "string",
                    "enum": [
                        "Water",
                        "Empty Cuvette",
                        "Nothing",
                        "Phosphate buffered saline",
                        "Urine",
                        "Fecal",
                        "Whole Blood",
                        "Sputum",
                        "Smear",
                        "Serum",
                        "Buffy Coat",
						"Plasma",
						"No Analyte"
                    ],
                    "abbreviation": [
                        "H2O",
                        "ECU",
                        "NCU",
                        "PBS",
                        "BMU",
                        "BMF",
                        "WBL",
                        "SPT",
                        "SMER",
                        "SRM",
                        "BFFC",
						"PLSM",
						"ANALYTE"
                    ],
                    "default": "No Analyte"
                },
                "tags": {
                    "title": "REFERENCE/TAGS",
                    "type": "array",
                    "ui:type": "TagBox",
                    "items": {
                        "type": "string",
                        "enum": [
                            "Proof of Concept",
                            "White Blood Cell",
                            "Natural White Blood Cell",
                            "Throat Swab",
                            "Nasal Swab",
                            "Spiked",
                            "Natural",
                            "USR5Min",
                            "USR20Min",
                            "10pcturine",
                            "20pcturine",
                            "100pcturine",
                            "FilterUrine",
                            "Slurry",
                            "Float"
                        ],
